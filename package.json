{
  "name": "iTrust-Auth-Widget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@mui/styled-engine-sc": "^5.12.0",
    "@mui/styles": "^5.13.2",
    "@mui/x-data-grid": "^6.19.2",
    "@mui/x-date-pickers": "^6.19.2",
    "@privateid/cryptonets-web-sdk": "3.3.3",
    "@react-keycloak/web": "^3.4.0",
    "@react-spring/web": "^9.7.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/parser": "^5.59.5",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "env-cmd": "^10.1.0",
    "lottie-react": "^2.4.0",
    "platform": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-papaparse": "^4.1.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.8",
    "styled-components": "^5.3.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prestart": "cp -R ./node_modules/@privateid/cryptonets-web-sdk/wasm public/&& cp -R ./node_modules/@privateid/cryptonets-web-sdk/workers public/",
    "start": "cross-env GENERATE_SOURCEMAP=false react-scripts start",
    "prebuild": "cp -R ./node_modules/@privateid/cryptonets-web-sdk/wasm public/ && cp -R ./node_modules/@privateid/cryptonets-web-sdk/workers public/",
    "build": "react-scripts build",
    "build:prod": "react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "build:widget": "parcel build src/index.js --no-source-maps --target widget  && cp build/static/css/*.css widget/static/css/ && cp build/static/js/*.js widget/static/js/ && cp build/index.html widget/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "parcel": "^2.12.0"
  }
}
